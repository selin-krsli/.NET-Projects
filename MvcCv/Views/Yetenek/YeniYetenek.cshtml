@using MvcCv.Models.Entity
@model TBL_SKILLS
@{
    ViewBag.Title = "YeniYetenek";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Yetenek Ekleme Sayfası</h4>
<br />

<form method="post">
    @Html.Label("Yetenek Açıklaması")
    @Html.TextBoxFor(s => s.Yetenek, new { @class = "form-control" })
    <br />
    @Html.Label("Derece")
    @Html.TextBoxFor(s => s.Oran, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

